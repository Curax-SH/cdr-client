{
  "interactiveLogin": false,
  "httpServer": {
    "type": "NettyWrapper"
  },
  "tokenProvider" : {
    "keyProvider" : {
      "algorithm" : "ES256"
    }
  },
  "tokenCallbacks": [
    {
      "issuerId": "test-tenant-client-id/oauth2/v2.0",
      "tokenExpiry":360,
      "requestMappings": [
        {
          "requestParam": "client_id",
          "match": "*",
          "claims": {
            "sub": "${clientId}",
            "azp": "${clientId}",
            "roles": [
              "CdrApi.ReadWrite.OwnedBy"
            ]
          }
        }
      ]
    }
  ]
}
