{
  "request": {
    "urlPattern": "/common/discovery/instance.*",
    "method": "GET"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json",
      "client-request-id": "{{request.headers.client-request-id}}"
    },
    "jsonBody": {
      "tenant_discovery_endpoint": "https://login.microsoftonline.com:443/common/.well-known/openid-configuration",
      "api-version": "1.1",
      "metadata": [
        {
          "preferred_network": "login.microsoftonline.com",
          "preferred_cache": "login.microsoftonline.com",
          "aliases": [
            "login.microsoftonline.com"
          ]
        }
      ]
    }
  }
}
